{
  "name": "@ibraheem4/linear-mcp",
  "version": "37.0.2",
  "description": "A Linear MCP server for interacting with Linear's API",
  "type": "module",
  "bin": {
    "@ibraheem4/linear-mcp": "./build/index.js",
    "linear-mcp": "./build/index.js"
  },
  "files": [
    "build"
  ],
  "scripts": {
    "build": "tsc && chmod +x build/index.js",
